{
    "name": "meskin/nailgun-php",
    "description": "A PHP Nailgun client",
    "type": "library",
    "license": "Apache-2.0",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Alireza Meskin",
            "email": "alireza.meskin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-sockets": "*",
        "psr/http-message": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Nailgun\\": "src/Nailgun" }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.8",
        "phpstan/phpstan-strict-rules": "^0.11.1",
        "phpunit/phpunit": "^8.2"
    }
}
