{
    "name": "meskin/nailgun-php",
    "description": "A PHP Nailgun client",
    "type": "library",
    "license": "Apache-2.0",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Alireza Meskin",
            "email": "alireza.meskin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-sockets": "*"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.0"
        }
    },
    "autoload": {
        "psr-4": { "Nailgun\\": "src/Nailgun" }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.9.2",
        "phpstan/phpstan-strict-rules": "^0.9.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Nailgun\\Tests\\": "tests/Nailgun"
        }
    }
}
